{
  "name": "portometro",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.3.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.10.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "express": "^4.17.1",
    "node-fetch": "^3.0.0",
    "nodemon": "^2.0.14",
    "onoff": "^6.0.3",
    "pigpio": "^3.3.1",
    "react": "^17.0.2",
    "react-animated-text": "^0.1.4",
    "react-d3-speedometer": "^1.0.1",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-text-transition": "^1.3.0",
    "typescript": "^4.4.4",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\" \"yarn server-start\"",
    "server-start": "nodemon server/server.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "concurrently": "^6.3.0",
    "electron": "^15.1.2",
    "electron-builder": "^22.13.1",
    "wait-on": "^6.0.0"
  },
  "main": "public/electron.js",
  "homepage": "./"
}
